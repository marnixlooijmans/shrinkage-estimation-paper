   "source": [
    "n_s = 21\n",
    "n_b = 18\n",
    "n_p = 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "id": "d9c20a5a",
   "metadata": {},
   "outputs": [],
   "source": [
    "A = 2/((n_s-n_b-1)*(n_s-n_b-4))\n",
    "B = (n_s-n_b-2)/((n_s-n_b-1)*(n_s-n_b-4))\n",
    "m_1 = (1+B*(n_b-n_p))/(1+A+B*(n_p+1))"
   ]
  },
